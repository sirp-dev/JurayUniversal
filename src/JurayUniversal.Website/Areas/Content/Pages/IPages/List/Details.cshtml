@page
@model JurayUniversal.Website.Areas.Content.Pages.IPages.List.DetailsModel

@{
    ViewData["Title"] = "PAGE LIST Details";
}

<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">DETAILS PAGE SECTION LIST</h3>

    </div>
    <!-- /.card-header -->

    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.PageSectionList.VideoUrl)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.PageSectionList.VideoUrl)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.PageSectionList.VideoKey)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.PageSectionList.VideoKey)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.PageSectionList.ImageUrl)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.PageSectionList.ImageUrl)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.PageSectionList.ImageKey)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.PageSectionList.ImageKey)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.PageSectionList.Title)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.PageSectionList.Title)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.PageSectionList.MiniTitle)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.PageSectionList.MiniTitle)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.PageSectionList.Description)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.PageSectionList.Description)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.PageSectionList.Disable)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.PageSectionList.Disable)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.PageSectionList.PageSection)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.PageSectionList.PageSection.Description)
                    </dd>
                </dl>
            </div>
        </div>
        <!-- /.card-body -->
        <div class="card-footer">
            <a href="javascript:history.back()" class="btn bg-gradient-warning btn-sm">Back</a>

        </div>
    </div>
</div>