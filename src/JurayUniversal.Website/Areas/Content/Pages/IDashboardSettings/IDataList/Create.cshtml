@page
@model JurayUniversal.Website.Areas.Content.Pages.IDashboardSettings.IDataList.CreateModel

@{
    ViewData["Title"] = "Create";
}

<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">DETAILS DASHBOARD DATA LIST</h3>

    </div>
    <!-- /.card-header -->

    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                <form method="post">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="DashboardDataList.DataIdQuery" class="control-label"></label>
                        <select asp-for="DashboardDataList.DataIdQuery" class="form-control" asp-items="ViewBag.CompanyProgramId">
                            <option value="">Choose</option>
                            <option value="0">Users</option>
                            <option value="00">Carrer</option>
                            <option value="000">Contact Us</option>
                        </select>

                    </div>

                    
                    <div class="form-group">
                        <label asp-for="DashboardDataList.DashboardDataId" class="control-label"></label>
                        <select asp-for="DashboardDataList.DashboardDataId" class="form-control" asp-items="ViewBag.DashboardDataId"></select>
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Submit" class="btn btn-primary" />
                        <a asp-page="./Index" class="btn bg-gradient-warning btn-sm" style="float:right;">Back</a>

                    </div>
                </form>
            </div>
        </div>


        <!-- /.row -->
    </div>
    <!-- /.card-body -->
    <div class="card-footer">
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
