@page
@model JurayUniversal.Website.Areas.Content.Pages.IPrograms.IProgramUser.CreateModel

@{
    ViewData["Title"] = "Create";
}


<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">NEW PROGRAM USER</h3>

    </div>
    <!-- /.card-header -->

    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                <form method="post">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="ProgramUser.UserId" class="control-label"></label>
                        <select asp-for="ProgramUser.UserId" class="form-control" asp-items="ViewBag.UserId"></select>
                    </div>
                    <div class="form-group">
                        <label asp-for="ProgramUser.CompanyProgramId" class="control-label"></label>
                        <select asp-for="ProgramUser.CompanyProgramId" class="form-control" asp-items="ViewBag.CompanyProgramId"></select>
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Submit" class="btn btn-primary" />
                        <a asp-page="./Index" class="btn bg-gradient-warning btn-sm" style="float:right;">Back</a>

                    </div>
                </form>
            </div>
        </div>


        <!-- /.row -->
    </div>
    <!-- /.card-body -->
    <div class="card-footer">
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
