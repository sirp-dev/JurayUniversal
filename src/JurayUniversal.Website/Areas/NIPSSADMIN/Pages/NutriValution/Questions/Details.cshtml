@page
@model JurayUniversal.Website.Areas.NIPSSADMIN.Pages.NutriValution.Questions.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">DETAILS NUTRITION LIST</h3>

    </div>
    <!-- /.card-header -->

    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.NutritionCategoryList.Question)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.NutritionCategoryList.Question)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.NutritionCategoryList.NutritionCategory)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.NutritionCategoryList.NutritionCategory.Name)
                    </dd>
                </dl>
            </div>
            <div>
                <a asp-page="./Edit" asp-route-id="@Model.NutritionCategoryList.Id">Edit</a> 
                <a asp-page="./Index">Back to List</a>
            </div>
        </div>
    </div>
</div>
