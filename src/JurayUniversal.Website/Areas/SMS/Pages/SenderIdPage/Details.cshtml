@page
@model JurayUniversal.Website.Areas.SMS.Pages.SenderIdPage.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">DETAILS SMS SENDER ID</h3>

    </div>
    <!-- /.card-header -->

    <div class="card-body">
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.SenderId.SenderIdStatus)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.SenderId.SenderIdStatus)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.SenderId.Title)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.SenderId.Title)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.SenderId.Date)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.SenderId.Date)
            </dd>
        </dl>
    </div>
    <!-- /.card-body -->
    <div class="card-footer">
        <a href="javascript:history.back()" class="btn bg-gradient-warning btn-sm">Back</a>

    </div>
</div>
