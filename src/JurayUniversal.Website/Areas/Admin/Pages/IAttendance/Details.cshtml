@page
@model JurayUniversal.Website.Areas.Admin.Pages.IAttendance.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">DETAILS ATTENANCE</h3>

    </div>
    <!-- /.card-header -->

    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Attendance.User)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Attendance.User.GetFullName())
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Attendance.StartTime)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Attendance.StartTime)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Attendance.ArivalPeriodStatus)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Attendance.ArivalPeriodStatus)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Attendance.CloseTime)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Attendance.CloseTime)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Attendance.ClosePeriodStatus)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Attendance.ClosePeriodStatus)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Attendance.Note)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Attendance.Note)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Attendance.AttendanceStatus)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Attendance.AttendanceStatus)
                    </dd>

                </dl>
            </div>
        </div>
        <!-- /.card-body -->
        <div class="card-footer">
            <a href="javascript:history.back()" class="btn bg-gradient-warning btn-sm">Back</a>
            <a class="btn bg-danger btn-sm" asp-page="./Edit" asp-route-id="@Model.Attendance.Id">Edit</a>

        </div>
    </div>
</div>