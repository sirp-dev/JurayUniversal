@page
@model JurayUniversal.Website.Areas.Admin.Pages.IRequest.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">DETAILS CLIENT REQUEST</h3>

    </div>
    <!-- /.card-header -->

    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ClientRequest.Name)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.ClientRequest.Name)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ClientRequest.Phone)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.ClientRequest.Phone)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ClientRequest.Email)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.ClientRequest.Email)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ClientRequest.Subject)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.ClientRequest.Subject)
                    </dd>

                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ClientRequest.Message)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.ClientRequest.Message)
                    </dd>
                </dl>
            </div>
        </div>
        <!-- /.card-body -->
        <div class="card-footer">
            <a href="javascript:history.back()" class="btn bg-gradient-warning btn-sm">Back</a>

        </div>
    </div>
</div>