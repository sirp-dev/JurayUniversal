@page
@model JurayUniversal.Website.Areas.Admin.Pages.CM.InfoModel

@{
    ViewData["Title"] = "Info";
}


<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title"> @Html.DisplayFor(model => model.CompanyProgram.Title) UNDER @Html.DisplayFor(model => model.CompanyProgram.CompanyProgramCategory.Title)</h3>

    </div>
    <!-- /.card-header -->

    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                <dl class="row">

                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.CompanyProgram.Description)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.CompanyProgram.Description)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.CompanyProgram.Address)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.CompanyProgram.Address)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.CompanyProgram.State)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.CompanyProgram.State)
                    </dd>

                </dl>
            </div>

        </div>
    </div> 
    <div class="card-footer">

        <a href="javascript:history.back()" class="btn bg-gradient-warning btn-sm">Back</a>

    </div>
</div>
