@page
@model JurayUniversal.Website.Areas.Admin.Pages.XProjectPage.XM.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">DETAILS PROJECT MESSAGE</h3>

    </div>
    <!-- /.card-header -->

    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.XProjectMessage.Message)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.XProjectMessage.Message)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.XProjectMessage.CreateDate)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.XProjectMessage.CreateDate)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.XProjectMessage.RepliedDate)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.XProjectMessage.RepliedDate)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.XProjectMessage.Sender)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.XProjectMessage.Sender.Id)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.XProjectMessage.Receiver)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.XProjectMessage.Receiver.Id)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.XProjectMessage.XProject)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.XProjectMessage.XProject.Description)
                    </dd>
                </dl>
            </div>
        </div>
        <!-- /.card-body -->
        <div class="card-footer">
            <a href="javascript:history.back()" class="btn bg-gradient-warning btn-sm">Back</a>

        </div>
    </div>
</div>
