@page
@model JurayUniversal.Website.Areas.Admin.Pages.XProjectPage.XS.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">DETAILS PROJECT SECTION</h3>

    </div>
    <!-- /.card-header -->

    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                <div>
                    <hr />
                    <dl class="row">
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.XProjectSection.Title)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.XProjectSection.Title)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.XProjectSection.Description)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.XProjectSection.Description)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.XProjectSection.FullDescription)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.XProjectSection.FullDescription)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.XProjectSection.CreatedDate)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.XProjectSection.CreatedDate)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.XProjectSection.StartDate)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.XProjectSection.StartDate)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.XProjectSection.DateToStartTesting)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.XProjectSection.DateToStartTesting)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.XProjectSection.Priority)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.XProjectSection.Priority)
                        </dd>
                    </dl>
                </div>
            </div>
            <!-- /.card-body -->
            <div class="card-footer">
                <a href="javascript:history.back()" class="btn bg-gradient-warning btn-sm">Back</a>

            </div>
        </div>
    </div>
</div>