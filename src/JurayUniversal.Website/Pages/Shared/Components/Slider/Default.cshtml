@model ICollection<JurayUniversal.Domain.Models.Slider>
@using System.Text.RegularExpressions;
@{
    Domain.Models.Setting xsetting = ViewBag.setting;
    Domain.Models.SuperSetting supersetting = ViewBag.superx;
    Regex regex = new Regex("[^a-zA-Z0-9]");
}


@if (supersetting.TemplateLayoutKey == "Layout1111")
{

    if (supersetting.SliderTemplateKey == "Slider1111")
    {
        <!-- Slider START -->
        <div class="swiper-main-slider-fade swiper-container">
            <div class="swiper-wrapper">
                @foreach (var item in Model)
                {
                    <!-- Slide 1 Start -->
                    <div class="swiper-slide" style="background-image:url(@item.Url);">
                        <!-- <div class="medium-overlay"></div> -->
                        <div class="container">
                            <div class="slider-content left-holder">
                                <h2 class="animated fadeInDown">
                                    @Html.Raw(item.Title)
                                </h2>
                                <div class="row">
                                    <div class="col-md-6 col-sm-12 col-12">
                                        <p class="animated fadeInDown">
                                            @Html.Raw(item.Text)
                                        </p>
                                    </div>
                                </div>
                                <div class="animated fadeInUp mt-30">

                                    @if (item.ButtonLink == "0")
                                    {
                                        <a href="#" class="dark-button button-md">@Html.Raw(item.ButtonText)</a>
                                    }
                                    else if (item.ButtonLink == "000")
                                    {
                                        <a asp-page="/Contact" class="dark-button button-md">@Html.Raw(item.ButtonText)</a>
                                    }
                                    else if (item.ButtonLink != "0" && item.ButtonLink == "000")
                                    {
                                        <a asp-page="/Source" asp-route-id="@item.ButtonLink" class="dark-button button-md">@Html.Raw(item.ButtonText)</a>
                                    }

                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Slide 1 End -->
                }
            </div>
            <!-- Add Arrows -->
            <div class="swiper-button-next"></div>
            <div class="swiper-button-prev"></div>
            <!-- Add Pagination -->
            <div class="swiper-pagination"></div>
        </div>
        <!-- Slider END -->
    }
    else if (supersetting.SliderTemplateKey == "Slider2222")
    {

        if (Model.Count() > 0)
        {

            foreach (var item in Model.Take(1))
            {
                <div class="main-video-section">
                    <div class="video-area" id="video-area">
                        <div class="player" id="main-video-play"
                             data-property="{videoURL:'@item.IsVideo', containment:'#video-area', showControls:false, autoPlay:true, zoom:0, loop:true, mute:true, startAt:7, opacity:1, quality:'low',}"></div>
                        <div class="main-video-overlay">
                            <div class="main-video-content">
                                <div class="container">
                                    <div class="white-color">
                                        <h3><strong>@Html.Raw(item.Title)</strong></h3>
                                        <div class="mt-15">
                                            <h6>
                                                @Html.Raw(item.Text)
                                            </h6>
                                        </div>
                                        @if (item.ButtonLink == "0")
                                        {
                                            <a href="#" class="primary-button button-md mt-30">@Html.Raw(item.ButtonText)</a>
                                        }
                                        else if (item.ButtonLink == "000")
                                        {
                                            <a asp-page="/Contact" class="primary-button button-md mt-30">@Html.Raw(item.ButtonText)</a>
                                        }
                                        else if (item.ButtonLink != "0" && item.ButtonLink == "000")
                                        {
                                            <a asp-page="/Source" asp-route-id="@item.ButtonLink" class="primary-button button-md mt-30">@Html.Raw(item.ButtonText)</a>
                                        }
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }
        }

    }
    else if (supersetting.SliderTemplateKey == "Slider3333")
    {
        if (Model.Count() > 0)
        {
            <!-- Parallax Slider START -->
            <div class="swiper-container" id="swiper-parallax">
                @{
                    string url = Model.Take(1).FirstOrDefault().Url;
                }
                <div class="parallax-bg" style="background-image:url(@url)" data-swiper-parallax="-23%"></div>
                <div class="swiper-wrapper">
                    @foreach (var item in Model)
                    {
                        <!-- Slide 1 Start -->
                        <div class="swiper-slide">
                            <div class="container">
                                <div class="slider-content left-holder">
                                    <h2 class="animated fadeInDown">
                                        @Html.Raw(item.Title)
                                    </h2>
                                    <div class="row">
                                        <div class="col-md-6 col-sm-12 col-12">
                                            <p class="animated fadeInDown">
                                                @Html.Raw(item.Text)
                                            </p>
                                        </div>
                                    </div>
                                    <div class="animated fadeInUp mt-30">
                                        @if (item.ButtonLink == "0")
                                        {
                                            <a href="#" class="dark-button button-md">@Html.Raw(item.ButtonText)</a>
                                        }
                                        else if (item.ButtonLink == "000")
                                        {
                                            <a asp-page="/Contact" class="dark-button button-md">@Html.Raw(item.ButtonText)</a>
                                        }
                                        else if (item.ButtonLink != "0" && item.ButtonLink == "000")
                                        {
                                            <a asp-page="/Source" asp-route-id="@item.ButtonLink" class="dark-button button-md">@Html.Raw(item.ButtonText)</a>
                                        }
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- Slide 1 End -->
                    }
                </div>

                <!-- Add Navigation -->
                <div class="swiper-button-prev swiper-button-white"></div>
                <div class="swiper-button-next swiper-button-white"></div>
            </div>
            <!-- Parallax Slider END -->
        }
    }
