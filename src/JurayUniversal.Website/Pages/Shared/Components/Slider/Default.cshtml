                                                                    }
                                                                    else if (item.ButtonLink != "0" && item.ButtonLink == "000")
                                                                    {
                                                                        <a asp-page="/Source" asp-route-id="@item.ButtonLink" class="gen-button" tabindex="0">
                                                                            <span class="text">
                                                                                @Html.Raw(item.ButtonText)
                                                                            </span>
                                                                        </a>

                                                                    }

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                }
                                <!-- Slider Items -->
                            </div>
                            <div class="slider slider-nav">

                                @foreach (var item in Model)
                                {
                                    <div class="slider-nav-contain">
                                        <div class="gen-nav-img">
                                            <img src="@item.Url" alt="@item.Title">
                                        </div>
                                        <div class="movie-info">
                                            <h3>@Html.Raw(item.Title)</h3>
                                            <div class="gen-movie-meta-holder">
                                                <ul>
                                                    <li></li>
                                                    <li></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                }


                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    }

}

@if (xsetting.EnableBreakingNewsRibon == true && xsetting.RibonPosition == Domain.Enum.Enum.RibonPosition.AfterSlider)
{
    @await Component.InvokeAsync("NewsRibon")
}