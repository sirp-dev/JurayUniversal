<!DOCTYPE html>
<html lang="en">
<head>
    @await Component.InvokeAsync("WebTitle")
    @await Component.InvokeAsync("Redirect")

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link href="~/Layout/RedBiz-template/css/animate.css" rel="stylesheet" />

    <link href="~/Layout/RedBiz-template/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Font-Awesome -->
    @*<link rel="stylesheet" type="text/css" href="~/Layout/RedBiz-template/css/font-awesome.css">*@
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA==" crossorigin="anonymous" referrerpolicy="no-referrer" />

    <!-- Icomoon -->
    <link rel="stylesheet" type="text/css" href="~/Layout/RedBiz-template/css/icomoon.css">

    <!-- Slider -->
    <link rel="stylesheet" type="text/css" href="~/Layout/RedBiz-template/css/swiper.min.css">

    <link rel="stylesheet" type="text/css" href="~/Layout/RedBiz-template/css/slider.css">

    <!-- Animate.css -->
    <link rel="stylesheet" href="~/Layout/RedBiz-template/css/animate.css">

    <!-- Revolution Slider  -->
    <link rel="stylesheet" href="~/Layout/RedBiz-template/css/rev-settings.css">
    <link rel="stylesheet" href="~/Layout/RedBiz-template/css/slider.css">

    <!-- Color Switcher -->
    <link rel="stylesheet" type="text/css" href="~/Layout/RedBiz-template/css/switcher.css">

    <!-- Owl Carousel  -->
    <link rel="stylesheet" href="~/Layout/RedBiz-template/css/owl.carousel.css">

    <!-- Main Styles -->
    <link rel="stylesheet" type="text/css" href="~/Layout/RedBiz-template/css/default.css">


    <!-- Fonts Google -->
    <link href="https://fonts.googleapis.com/css?family=Fira+Sans:100,200,300,400,500,600,700,800,900" rel="stylesheet">
    <link href="~/customcss/maquee1.css" rel="stylesheet" />

    @RenderSection("MainStyle", required: false)


    @await Component.InvokeAsync("WebsiteCSS")

</head>
<body>
    <!-- Preloader Start-->
    @await Component.InvokeAsync("Reloader")
    <!-- Preloader End -->
    @await Component.InvokeAsync("Menu")

    @RenderBody()

    @await Component.InvokeAsync("Footer")


    <!-- Scroll to top button Start -->
    <a href="#" class="scroll-to-top"><i class="fa fa-angle-up" aria-hidden="true"></i></a>
    <!-- Scroll to top button End -->

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.13.2/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button)
    </script>
    <!-- Bootstrap 4 -->
    <script src="~/Layout/RedBiz-template/js/bootstrap.min.js"></script>
    <!-- Jquery -->
    <!--Popper JS-->
    <script src="~/Layout/RedBiz-template/js/popper.min.js"></script>

    <!--Popper JS-->
    <script src="~/Layout/RedBiz-template/js/popper.min.js"></script>




    <!-- Owl Carousel-->
    <script src="~/Layout/RedBiz-template/js/owl.carousel.js"></script>

    <!-- Navbar JS -->
    <script src="~/Layout/RedBiz-template/js/navigation.js"></script>
    <script src="~/Layout/RedBiz-template/js/navigation.fixed.js"></script>

    <!-- Wow JS -->
    <script src="~/Layout/RedBiz-template/js/wow.min.js"></script>

    <!-- Countup -->
    <script src="~/Layout/RedBiz-template/js/jquery.counterup.min.js"></script>
    <script src="~/Layout/RedBiz-template/js/waypoints.min.js"></script>

    <!-- Tabs -->
    <script src="~/Layout/RedBiz-template/js/tabs.min.js"></script>

    <!-- Yotube Video Player -->
    <script src="~/Layout/RedBiz-template/js/jquery.mb.YTPlayer.min.js"></script>

    <!-- Swiper Slider -->
    <script src="~/Layout/RedBiz-template/js/swiper.min.js"></script>

    <!-- Isotop -->
    <script src="~/Layout/RedBiz-template/js/isotope.pkgd.min.js"></script>

    <!-- Switcher JS -->
    <script src="~/Layout/RedBiz-template/js/switcher.js"></script>

    <!-- Modernizr -->
    <script src="~/Layout/RedBiz-template/js/modernizr.js"></script>
    <script src="~/Layout/RedBiz-template/js/revolution/jquery.themepunch.tools.min.js"></script>
    <script src="~/Layout/RedBiz-template/js/revolution/jquery.themepunch.revolution.min.js"></script>
    <script src='~/Layout/RedBiz-template/js/revolution/revolution.addon.slicey.min.js?ver=1.0.0'></script>
    <script src="~/Layout/RedBiz-template/js/revolution/revolution.extension.actions.min.js"></script>
    <script src="~/Layout/RedBiz-template/js/revolution/revolution.extension.kenburn.min.js"></script>
    <script src="~/Layout/RedBiz-template/js/revolution/revolution.extension.layeranimation.min.js"></script>
    <script src="~/Layout/RedBiz-template/js/revolution/revolution.extension.migration.min.js"></script>
    <script src="~/Layout/RedBiz-template/js/revolution/revolution.extension.slideanims.min.js"></script>
    <!-- Google Map -->
    <script src="~/Layout/RedBiz-template/js/map.js"></script>
    <!-- Main JS -->
    <script src="~/Layout/RedBiz-template/js/main.js"></script>
    @await Component.InvokeAsync("Modal")

    @await Component.InvokeAsync("CustomeJavascript")


    @RenderSection("Scripts", required: false)
    @*@await RenderSectionAsync("Scripts", required: false)*@

</body>
</html>