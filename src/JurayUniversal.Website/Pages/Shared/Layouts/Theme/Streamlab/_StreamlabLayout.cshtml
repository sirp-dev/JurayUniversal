<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">

    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    @await Component.InvokeAsync("WebTitle")
    @await Component.InvokeAsync("Redirect")
    <!-- CSS bootstrap-->
    <link rel="stylesheet" href="~/css/bootstrap.min.css" />
    <link href="~/css/style2.css" rel="stylesheet" />
    <!--  Style -->
    <link rel="stylesheet" href="~/css/style.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA==" crossorigin="anonymous" referrerpolicy="no-referrer" />

    <!--  Responsive -->
    <link rel="stylesheet" href="~/css/responsive.css" />
    <link href="~/customcss/maquee1.css" rel="stylesheet" />

    @RenderSection("MainStyle", required: false)


    @await Component.InvokeAsync("WebsiteCSS")
</head>

<body>
    <!-- Preloader Start-->
    @await Component.InvokeAsync("Reloader")
    <!-- Preloader End -->
    @await Component.InvokeAsync("Menu")


     

    @RenderBody()

    @await Component.InvokeAsync("Footer")

    <!-- footer End -->
    <!-- Back-to-Top start -->
    <div id="back-to-top">
        <a class="top" id="top" href="#top"> <i class="ion-ios-arrow-up"></i> </a>
    </div>
    <!-- Back-to-Top end -->
    <!-- js-min -->
    <script src="~/js/jquery-3.6.0.min.js"></script>
    <script src="~/js/asyncloader.min.js"></script>
    <!-- JS bootstrap -->
    <script src="~/js/bootstrap.min.js"></script>
    <!-- owl-carousel -->
    <script src="~/js/owl.carousel.min.js"></script>
    <!-- counter-js -->
    <script src="~/js/jquery.waypoints.min.js"></script>
    <script src="~/js/jquery.counterup.min.js"></script>
    <!-- popper-js -->
    <script src="~/js/popper.min.js"></script>
    <script src="~/js/swiper-bundle.min.js"></script>
    <!-- Iscotop -->
    <script src="~/js/isotope.pkgd.min.js"></script>

    <script src="~/js/jquery.magnific-popup.min.js"></script>

    <script src="~/js/slick.min.js"></script>
    <script src="~/js/asyncloader.min.js"></script>








    <script src="~/js/streamlab-core.js"></script>

    <script src="~/js/script.js"></script>

    @await Component.InvokeAsync("Modal")

    @await Component.InvokeAsync("CustomeJavascript")


    @RenderSection("Scripts", required: false)
</body>

</html>