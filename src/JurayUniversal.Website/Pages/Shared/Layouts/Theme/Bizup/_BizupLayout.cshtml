    <!-- magnific popup js -->
    <script src="~/Layout/bizup/assets/js/jquery.magnific-popup.min.js"></script>
    <!-- Nivo slider js -->
    <script src="~/Layout/bizup/assets/inc/custom-slider/js/jquery.nivo.slider.js"></script>
    <!-- contact form js -->
    <script src="~/Layout/bizup/assets/js/contact.form.js"></script>
    <!-- main js -->
    <script src="~/Layout/bizup/assets/js/main.js"></script>
    @await Component.InvokeAsync("Modal")
    @await Component.InvokeAsync("CustomeJavascript")

    @RenderSection("Scripts", required: false)


</body>
</html>