    <link rel="stylesheet" href="~/Layout/Senatory/assets/css/hover-min.css" />
    <!-- Main Stylesheet -->
    <link rel="stylesheet" href="~/Layout/Senatory/assets/css/style.css" />
    <!-- responsive Stylesheet -->
    <link rel="stylesheet" href="~/Layout/Senatory/assets/css/responsive.css" />
    <link href="~/customcss/maquee1.css" rel="stylesheet" />

    @RenderSection("MainStyle", required: false)


