    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <!-- animate -->
    <link rel="stylesheet" href="~/Layout/Senatory/assets/css/animate.css" />
    <!-- bootstrap -->
    <link rel="stylesheet" href="~/Layout/Senatory/assets/css/bootstrap.min.css" />
    <!-- magnific popup -->
    <link rel="stylesheet" href="~/Layout/Senatory/assets/css/magnific-popup.css" />
    <!-- owl carousel -->
    <link rel="stylesheet" href="~/Layout/Senatory/assets/css/owl.carousel.min.css" />
    <!-- Slick Slider -->
    <link rel="stylesheet" href="~/Layout/Senatory/assets/css/slick.css">
