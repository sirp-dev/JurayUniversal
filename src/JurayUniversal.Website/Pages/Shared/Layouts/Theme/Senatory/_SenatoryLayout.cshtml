    <!-- slick Slider -->
    <script src="~/Layout/Senatory/assets/js/slick.min.js"></script>
    <!-- waypoint -->
    <script src="~/Layout/Senatory/assets/js/waypoints.min.js"></script>
    <!-- counterup -->
    <script src="~/Layout/Senatory/assets/js/jquery.counterup.min.js"></script>
